public class TestSort {

	public static void main(String[] args) {
		int arr[] = {45,1,34,2,78,9,5,90,4};
		SelectionSort.selectionSort(arr);
		System.out.println("========================================================");
		MergeSort.mergeSort(arr, 0, arr.length-1);

	}

}
