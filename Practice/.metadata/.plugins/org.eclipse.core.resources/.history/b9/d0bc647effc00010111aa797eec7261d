package com.demo.queue;

public class QueueUsingLinkedList {

	Node front;
	class Node{
		int data;
		Node next;
		Node rear;
		
		
		public Node(int data) {
			this.data = data;
		}
	}
	public QueueUsingLinkedList() {
		front = null;
	}
	
	
	public boolean isEmpty() {
		if (front==null) {
			return true;	
		}
		return false;
	}
	
	
	public void enQueue(int data) {
		Node newnode = new Node(data);
		if (isEmpty()) {
			front=newnode;
			newnode.next = null;
		}else {
			Node temp = front;
			while (temp.next!=null) {
				
			}
		}
	}
}
