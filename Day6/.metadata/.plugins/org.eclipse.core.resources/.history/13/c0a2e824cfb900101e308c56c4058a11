package com.demo.linkedList;

import org.w3c.dom.Node;

public class SinglyLinkedList {
	Node head;
	class Node{
		int data;
		Node next;
		public Node(int data) {
			this.data = data;
			next = null;
		}
	}
	
	public SinglyLinkedList() {
		head = null;
	}
	
	
	public void addByNum(int value, int num) {
		Node newnode = new Node(value);
	}
	
	
	
}
