		newArr[1]=14;
		newArr[2]=15;
		newArr[3]=16;
		newArr[4]=17;
		newArr[5]=18;
		newArr[6]=19;
		

		MyArray ma3 = new MyArray(newArr);
		System.out.println("Capacity: "+ma3.getCapacity());
		System.out.println("Size: "+ma3.getSize());
		ma3.display();
		
